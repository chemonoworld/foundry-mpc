{
  "name": "foundry-mpc-ts",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "version": "0.1.0",
  "packageManager": "yarn@4.7.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chemonoworld/foundry-mpc.git",
    "directory": "foundry-mpc-ts"
  },
  "scripts": {
    "clean": "del-cli dist",
    "build": "yarn clean && tsc",
    "start": "yarn build && yarn copy:wasm && node ./dist/src/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "typecheck": "tsc --noEmit",
    "copy:wasm": "del-cli pkg dist/pkg && cp -R ../foundry-mpc-wasm/pkg-bundler ./pkg && mkdir -p dist && cp -R ./pkg ./dist/pkg",
    "test": "jest"
  },
  "dependencies": {
    "@keplr-ewallet/stdlib-js": "^0.0.2-rc.18",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "del-cli": "*",
    "jest": "*",
    "ts-jest": "*",
    "typescript": "*"
  },
  "gitHead": "0960908938cd494a3b11af557553dba8fb53b674"
}
